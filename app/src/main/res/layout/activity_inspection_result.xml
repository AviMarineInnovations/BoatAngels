<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="in.avimarine.boatangels.activities.InspectionResultActivity"
  tools:visibility="visible">

  <ImageView
    android:id="@+id/moored_boat_bowlines"
    android:layout_width="0dp"
    android:layout_height="60dp"
    android:layout_marginEnd="85dp"
    android:contentDescription="@string/boat_drawing_content_description"
    app:layout_constraintEnd_toEndOf="@+id/moored_boat_body"
    app:layout_constraintStart_toStartOf="@+id/moored_boat_body"
    app:layout_constraintTop_toBottomOf="@+id/inspection_subtitle"
    app:srcCompat="@drawable/ic_moored_sailing_boat_bowlines"
    android:layout_marginRight="85dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"
    />

  <ImageView
    android:id="@+id/moored_boat_body"
    android:layout_width="0dp"
    android:layout_height="250dp"
    android:contentDescription="@string/boat_drawing_content_description"
    app:layout_constraintBottom_toTopOf="@+id/moored_boat_sternlines"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.502"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/moored_boat_bowlines"
    app:layout_constraintVertical_chainStyle="spread_inside"
    app:srcCompat="@drawable/ic_moored_sailing_boat_body_sails"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"
    />
  <ImageView
    android:id="@+id/moored_boat_sternlines"
    android:layout_width="0dp"
    android:layout_height="45dp"
    android:layout_marginEnd="82dp"
    android:contentDescription="@string/boat_drawing_content_description"
    app:layout_constraintEnd_toEndOf="@+id/moored_boat_body"
    app:layout_constraintHorizontal_bias="0.43"
    app:layout_constraintStart_toStartOf="@+id/moored_boat_body"
    app:layout_constraintTop_toBottomOf="@+id/moored_boat_body"
    app:srcCompat="@drawable/ic_moored_sailing_boat_sternlines"
    android:layout_marginRight="82dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"/>

  <TextView
    android:id="@+id/message_linedEditText"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:background="@android:color/transparent"
    android:enabled="false"
    android:singleLine="false"
    android:visibility="gone"
    app:layout_constraintBottom_toTopOf="@+id/imageButton"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/moored_boat_sternlines"
    tools:visibility="visible"/>
  <in.avimarine.boatangels.CheckBoxTriState
    android:id="@+id/checkBox_stern"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintBottom_toBottomOf="@+id/moored_boat_sternlines"
    app:layout_constraintEnd_toEndOf="@+id/moored_boat_sternlines"
    app:layout_constraintStart_toStartOf="@+id/moored_boat_sternlines"
    app:layout_constraintTop_toTopOf="@+id/moored_boat_sternlines"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"/>
  <in.avimarine.boatangels.CheckBoxTriState
    android:id="@+id/checkBox_bow"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="5dp"
    app:layout_constraintBottom_toBottomOf="@+id/moored_boat_bowlines"
    app:layout_constraintEnd_toEndOf="@+id/moored_boat_bowlines"
    app:layout_constraintStart_toStartOf="@+id/moored_boat_bowlines"
    app:layout_constraintTop_toTopOf="@+id/moored_boat_bowlines"
    android:layout_marginRight="5dp"
    android:layout_marginLeft="8dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"/>
  <in.avimarine.boatangels.CheckBoxTriState
    android:id="@+id/checkBox_jib"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="160dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="80dp"
    android:checked="false"
    android:enabled="false"
    android:visibility="gone"
    app:layout_constraintBottom_toBottomOf="@+id/moored_boat_body"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.76"
    app:layout_constraintStart_toStartOf="@+id/moored_boat_body"
    app:layout_constraintTop_toBottomOf="@+id/moored_boat_bowlines"
    app:layout_constraintVertical_bias="0.469"
    tools:visibility="visible"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="80dp"/>
  <in.avimarine.boatangels.CheckBoxTriState
    android:id="@+id/checkBox_mainsail"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="60dp"
    android:layout_marginStart="24dp"
    android:layout_marginEnd="20dp"
    android:enabled="false"
    android:visibility="gone"
    app:layout_constraintBottom_toBottomOf="@+id/moored_boat_body"
    app:layout_constraintEnd_toEndOf="@+id/moored_boat_body"
    app:layout_constraintHorizontal_bias="0.65"
    app:layout_constraintStart_toStartOf="@+id/moored_boat_body"
    app:layout_constraintTop_toTopOf="@+id/moored_boat_body"
    app:layout_constraintVertical_bias="0.63"
    tools:visibility="visible"
    android:layout_marginLeft="24dp"
    android:layout_marginRight="20dp"/>
  <TextView
    android:id="@+id/inspection_title"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:textSize="18sp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_marginLeft="8dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"/>
  <TextView
    android:id="@+id/inspection_subtitle"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/inspection_title"
    android:layout_marginLeft="8dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"/>
  <ImageButton
    android:id="@+id/imageButton"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="4dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/message_linedEditText"
    app:srcCompat="@drawable/ic_send_black_24dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"
    android:contentDescription="@string/sendbutton_content_description"/>
  <ImageButton
    android:id="@+id/imageButton2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintBottom_toBottomOf="@+id/imageButton"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/imageButton"
    app:layout_constraintTop_toTopOf="@+id/imageButton"
    app:srcCompat="@drawable/ic_add_a_photo_black_24dp"
    android:enabled="false"
    android:visibility="gone"
    tools:visibility="visible"
    android:contentDescription="@string/addphotosbuton_content_description"/>

</android.support.constraint.ConstraintLayout>
